{
  "name": "go-to-safari",
  "version": "1.0.0",
  "description": "Launch safari from webview. Almost absolutely.",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "lint": "eslint ./src",
    "test": "nyc ./node_modules/.bin/ava -v",
    "prepublishOnly": "npm run build",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "standard": "standard index.js"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youheinakagawa/go-to-safari-js.git"
  },
  "keywords": [
    "npm",
    "node",
    "javascript",
    "safari",
    "webview"
  ],
  "author": "Youhei Nakagawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youheinakagawa/go-to-safari-js/issues"
  },
  "homepage": "https://github.com/youheinakagawa/go-to-safari-js#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1",
    "nyc": "^10.0.0",
    "ava": "^0.16.0",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "ghooks": "^1.2.4",
    "standard": "^8.4.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
  }
}